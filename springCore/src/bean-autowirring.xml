<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="product" class="springAutoWirring.Product">
		<property name="name" value="Iphone15"></property>
		<property name="productId" value="11231"></property>
	</bean>
	
	<bean id="product2" class="springAutoWirring.Product">
		<property name="name" value="Samsung"></property>
		<property name="productId" value="31531"></property>
	</bean>
	
	<bean id="order1" class="springAutoWirring.Orders" autowire="byName">
		<property name="orderId" value="101"></property>
		<property name="price" value="100000.00"></property>
	</bean>
	
	<bean id="order2" class="springAutoWirring.Orders">
		<property name="orderId" value="102"></property>
		<property name="price" value="50000.00"></property>
		<property name="Product" ref="product2"></property>
	</bean>
</beans>